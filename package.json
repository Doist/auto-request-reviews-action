{
  "name": "auto-request-reviews-action",
  "version": "1.0.0",
  "description": "GitHub Action that automatically requests reviews from members of a specified team",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "format": "prettier --write **/*.ts"
  },
  "keywords": [
    "github",
    "actions",
    "reviews",
    "pull-requests"
  ],
  "author": "Doist",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}